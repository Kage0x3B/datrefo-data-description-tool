{
    "name": "datrefo-data-description-tool",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "npm run generateMetadata && vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "vitest",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "generateMetadata": "mkdir -p ./src/lib/generated && npm run generateFhirMetadata && npm run generateFhirLocaleData",
        "generateFhirMetadata": "ts-node-esm -P tsconfig-tools.json tools/generateFhirResourceTypes.ts && ts-node-esm -P tsconfig-tools.json tools/generateFhirMetadata.ts",
        "generateFhirLocaleData": "mkdir -p ./src/lib/generated/i18n/fhir && ts-node-esm -P tsconfig-tools.json tools/generateFhirLocaleData.ts"
    },
    "devDependencies": {
        "@iconify/icons-fa6-solid": "^1.2.10",
        "@iconify/svelte": "^3.0.1",
        "@playwright/test": "^1.29.2",
        "@sveltejs/adapter-auto": "^1.0.1",
        "@sveltejs/adapter-cloudflare": "^1.0.3",
        "@sveltejs/kit": "^1.1.3",
        "@tailwindcss/typography": "^0.5.9",
        "@tsconfig/node16": "^1.0.3",
        "@types/file-saver": "^2.0.5",
        "@types/json-schema": "^7.0.11",
        "@types/node": "^18.11.18",
        "@types/prettier": "^2.7.2",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "autoprefixer": "^10.4.13",
        "change-case": "^4.1.2",
        "daisyui": "^2.47.0",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "file-saver": "^2.0.5",
        "jszip": "^3.10.1",
        "postcss": "^8.4.21",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.8.3",
        "prettier-plugin-svelte": "^2.9.0",
        "svelte": "^3.55.1",
        "svelte-check": "^3.0.2",
        "svelte-i18n": "^3.6.0",
        "svelte-preprocess": "^5.0.0",
        "tailwindcss": "^3.2.4",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.1",
        "typescript": "^4.9.4",
        "vite": "^4.0.4",
        "vitest": "^0.27.2"
    },
    "type": "module"
}
