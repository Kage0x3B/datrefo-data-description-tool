{
    "useTabs": false,
    "singleQuote": true,
    "editor.formatOnSave": true,
    "trailingComma": "none",
    "printWidth": 140,
    "tabWidth": 4,
    "semi": true,
    "bracketSpacing": true,
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
